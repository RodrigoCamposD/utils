{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "plugins": ["prettier"],
  "extends": ["prettier", "plugin:node/recommended", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "consistent-return": "off",
    "linebreak-style": ["error", "windows"],
    "quotes": ["error", "double"],
    "no-console": "off",
    "no-process-exit": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "req|res|next|val|" }]
  },
  "engines": {
    "node": ">=18.14.0"
  }
}
